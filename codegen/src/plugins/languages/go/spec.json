  "conventions": {
    "fileNaming": "snake-case",
    "structNaming": "pascal-case",
    "methodNaming": "pascal-case",
    "propertyNaming": "pascal-case",
    "constantNaming": "upper-snake-case",
    "typeNaming": "pascal-case",
    "interfaceSuffix": "Interface",
    "privatePrefix": "",
    "publicPrefix": ""
  },
  "templates": {
    "struct": {
      "pattern": ["type {name} struct {", "    {fields}", "}"]
    },
    "interface": {
      "pattern": ["type {name} interface {", "    {methods}", "}"]
    },
    "function": {
      "pattern": ["func {name}({parameters}) {return-type} {", "    {body}", "}"]
    },
    "method": {
      "pattern": ["func ({receiver}) {name}({parameters}) {return-type} {", "    {body}", "}"]
    },
    "goroutine": {
      "pattern": ["go func() {", "    {body}", "}()"]
    }
  },
  "features": {
    "goroutines": true,
    "channels": true,
    "interfaces": true,
    "embedding": true,
    "reflection": true,
    "garbageCollection": true,
