  "config": {
    "entry": "src/App.tsx",
    "styles": "styles.scss",
    "render": {
      "rootId": "root",
      "domModule": "react-dom/client",
      "reactModule": "react",
      "createRoot": "createRoot",
      "renderMethod": "render"
    },
    "fallbackProviders": [
      "https://cdn.skypack.dev/",
      "https://jspm.dev/"
    ],
    "providers": {
      "default": "https://unpkg.com",
      "esm": "https://esm.sh",
      "aliases": {
        "unpkg": "https://unpkg.com",
        "unpkg.com": "https://unpkg.com"
      }
    },
    "server": {
      "host": "0.0.0.0",
      "port": 4173,
      "logFile": "server.log",
      "jsonLimit": "1mb",
      "cacheControl": "no-store, no-cache, must-revalidate",
      "logTruncateLength": 4000,
      "paths": {
        "envScript": "/bootstrap/entrypoints/env.js",
        "proxy": "/proxy/unpkg",
        "clientLog": "/__client-log"
      }
    },
    "tools": [
      {
        "name": "@babel/standalone",
        "ci_provider": "/proxy/unpkg",
        "production_provider": "unpkg.com",
        "version": "7.26.0",
        "pathPrefix": "",
        "file": "babel.min.js",
        "global": "Babel"
      },
      {
        "name": "sass.js",
        "ci_provider": "/proxy/unpkg",
        "production_provider": "unpkg.com",
        "version": "0.11.1",
        "pathPrefix": "dist",
        "file": "sass.sync.js",
        "global": "Sass"
      }
    ],
    "modules": [
      {
        "name": "react",
        "ci_provider": "https://esm.sh/",
        "production_provider": "https://esm.sh/",
        "version": "19.2.3",
        "pathPrefix": "es2022",
        "file": "react.mjs",
        "global": "React",
        "format": "esm"
      },
      {
        "name": "react/jsx-runtime",
        "package": "react",
        "ci_provider": "https://esm.sh/",
        "production_provider": "https://esm.sh/",
        "version": "19.2.3",
        "pathPrefix": "es2022",
        "file": "jsx-runtime.mjs",
        "format": "esm"
      },
      {
        "name": "react-dom/client",
        "package": "react-dom",
        "ci_provider": "https://esm.sh/",
        "production_provider": "https://esm.sh/",
        "version": "19.2.3",
        "pathPrefix": "es2022",
        "file": "client.mjs",
        "global": "ReactDOM",
        "format": "esm"
      },
      {
        "name": "@mui/material",
        "ci_provider": "https://esm.sh/",
        "production_provider": "https://esm.sh/",
        "version": "7.3.6",
        "pathPrefix": "es2022",
        "file": "material.mjs",
        "global": "MaterialUI",
        "format": "esm"
      },
      {
        "name": "@emotion/react",
        "ci_provider": "https://esm.sh/",
        "production_provider": "https://esm.sh/",
        "version": "11.13.0",
        "pathPrefix": "es2022",
        "file": "react.mjs",
        "global": "emotionReact",
        "format": "esm"
      },
      {
        "name": "@emotion/styled",
        "ci_provider": "https://esm.sh/",
        "production_provider": "https://esm.sh/",
        "version": "11.13.0",
        "pathPrefix": "es2022",
        "file": "styled.mjs",
        "global": "emotionStyled",
        "format": "esm"
      }
    ],
    "dynamicModules": [
      {
        "prefix": "@mui/icons-material/",
        "ci_provider": "https://esm.sh/",
        "production_provider": "https://esm.sh/",
        "package": "@mui/icons-material",
        "version": "7.3.6",
        "pathPrefix": "es2022",
        "filePattern": "{icon}.mjs",
        "format": "esm",
        "provider": "https://esm.sh/"
      },
      {
        "prefix": "@mui/material/",
        "ci_provider": "https://esm.sh/",
        "production_provider": "https://esm.sh/",
        "package": "@mui/material",
        "version": "7.3.6",
        "pathPrefix": "es2022",
        "filePattern": "{icon}.mjs",
        "format": "esm",
        "provider": "https://esm.sh/"
      }
    ]
  },
  "templates": {
    "project": {
      "name": "RevolutionaryExample",
      "version": "1.0.0",
      "description": "An example project demonstrating RevolutionaryCodegen capabilities",
      "author": "Revolutionary Developer",
