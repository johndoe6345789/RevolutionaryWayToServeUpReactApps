      "repository": "https://github.com/revolutionary-codegen-example"
    },
    "enhanced": {
      "name": "RevolutionaryEnhancedDemo",
      "version": "1.0.0",
      "description": "A comprehensive demonstration project showcasing all revolutionary codegen enhancements",
      "author": "Revolutionary Developer",
      "license": "MIT",
      "repository": "https://github.com/revolutionary-enhanced-demo"
    },
    "multiLanguage": {
      "name": "RevolutionaryMultiLanguageDemo",
      "version": "1.0.0",
      "description": "A revolutionary multi-language project demonstrating Python, Java, C++, and JavaScript code generation with entry points, class registries, factory patterns, and builder patterns",
      "author": "Revolutionary Developer",
      "license": "MIT",
      "mainClass": "MultiLanguageApplication"
    }
  },
  "constants": {
    "maxNestingLevel": 5,
    "defaultAggregateType": "nested",
    "generatedCodeMarker": "Auto-generated by RevolutionaryCodegen",
    "defaultLanguage": "javascript",
    "functions": {
      "getTimestamp": "return new Date().toISOString();",
      "calculateVersion": "return Math.floor(Date.now() / 1000).toString();",
      "generateId": "return Date.now().toString(36) + Math.random().toString(36).substr(2);",
      "generateUuid": "return require('crypto').randomUUID();",
      "capitalize": "return str.charAt(0).toUpperCase() + str.slice(1);",
      "camelCase": "return str.replace(/-([a-z])/g, (g) => g[1].toUpperCase());",
      "kebabCase": "return str.replace(/([A-Z])/g, '-$1').toLowerCase();",
      "slugify": "return str.toLowerCase().replace(/[^a-z0-9]+/g, '-').replace(/-+/g, '-');"
    },
    "definitions": [
      {
        "name": "API_BASE_URL",
        "value": "https://api.example.com/v1",
        "type": "string",
        "description": "Base API URL for the application"
      },
      {
        "name": "APP_NAME",
        "value": "RevolutionaryEnhancedDemo",
        "type": "string",
        "description": "Application name"
      },
      {
        "name": "APP_VERSION",
        "value": "1.0.0",
        "type": "string",
        "description": "Application version"
      },
      {
        "name": "ENVIRONMENT",
        "value": "development",
        "type": "string",
        "description": "Current environment (development, staging, production)"
      },
      {
        "name": "MAX_RETRIES",
        "value": 3,
        "type": "number",
        "description": "Maximum retry attempts for operations"
      },
      {
        "name": "SESSION_TIMEOUT",
        "value": 3600,
        "type": "number",
        "description": "Session timeout in seconds"
      },
      {
        "name": "ORDER_TIMEOUT",
        "value": 300,
        "type": "number",
        "description": "Order processing timeout in seconds"
      }
    ]
  },
  "metadata": {
    "pluginGroups": [
      {
        "name": "core",
        "description": "Core functionality plugins for revolutionary codegen system",
        "category": "generation"
      },
      {
        "name": "validation",
        "description": "Validation plugins for ensuring code quality and compliance",
        "category": "analysis"
      },
      {
        "name": "utilities",
        "description": "Utility plugins for development productivity and code quality",
        "category": "utility"
      }
