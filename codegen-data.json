    "codegen": {
      "templates": {
        "businessClass": "Template for business logic classes with initialize/execute pattern",
        "dataClass": "Template for data classes with validation and type safety",
        "factoryClass": "Template for factory classes with dependency injection",
        "aggregateClass": "Template for aggregate classes with unlimited nesting",
        "serviceClass": "Template for service classes with error handling",
        "controllerClass": "Template for controller classes with request handling",
        "utilityClass": "Template for utility classes with helper functions"
      },
      "rules": {
        "enforceTwoMethodLimit": true,
        "requireDataclassPattern": true,
        "requireSingleConstructorParam": true,
        "requireBaseClassInheritance": true,
        "maxClassLines": 100,
        "maxMethodLines": 20,
        "enforceNamingConventions": true,
        "requireTypeScriptDefinitions": true,
        "requireDocumentation": true
      }
    }
  },
