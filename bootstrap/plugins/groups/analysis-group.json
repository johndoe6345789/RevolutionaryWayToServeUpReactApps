{
  "name": "AnalysisGroup",
  "description": "Analysis and code quality plugins",
  "category": "analysis",
  "enabled": true,
  "config": {
    "parallelExecution": true,
    "timeout": 60000,
    "reportFormat": "json"
  },
  "dependencies": [],
  "loadOrder": [
    "OOPPrinciplesPlugin",
    "DependencyAnalyzerPlugin",
    "CodeCoveragePlugin"
  ],
  "plugins": [
    {
      "name": "OOPPrinciplesPlugin",
      "version": "1.0.0",
      "enabled": true,
      "description": "Analyzes and enforces OOP principles",
      "dependencies": [],
      "config": {
        "maxClassLines": 100,
        "maxMethodLines": 20,
        "enableSOLIDAnalysis": true
      },
      "module": "plugins/oop-principles.plugin.js",
      "factory": "plugins/factories/oop-principles-factory.js",
      "dataClass": "plugins/data/oop-principles-data.js"
    },
    {
      "name": "DependencyAnalyzerPlugin",
      "version": "1.0.0",
      "enabled": true,
      "description": "Analyzes project dependencies",
      "dependencies": [],
      "config": {
        "includeDevDependencies": false,
        "maxDepth": 5,
        "outputFormat": "graph"
      },
      "module": "plugins/dependency-analyzer.plugin.js",
      "factory": "plugins/factories/dependency-analyzer-factory.js",
      "dataClass": "plugins/data/dependency-analyzer-data.js"
    },
    {
      "name": "CodeCoveragePlugin",
      "version": "1.0.0",
      "enabled": false,
      "description": "Generates code coverage reports",
      "dependencies": ["DependencyAnalyzerPlugin"],
      "config": {
        "threshold": 80,
        "excludePatterns": ["test/", "node_modules/"],
        "reportFormats": ["html", "json"]
      },
      "module": "plugins/code-coverage.plugin.js",
      "factory": "plugins/factories/code-coverage-factory.js",
      "dataClass": "plugins/data/code-coverage-data.js"
    }
  ],
  "metadata": {
    "created": "2025-12-22T10:00:00.000Z",
    "updated": "2025-12-22T10:00:00.000Z",
    "author": "RWTRA System",
    "version": "1.0.0",
    "tags": ["analysis", "quality", "oop", "dependencies"]
  }
}
